{
  "name": "@10xjs/form",
  "version": "0.1.7",
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:cjs && npm run build:module",
    "build:clean": "npx rimraf lib module types",
    "build:cjs": "npx rollup -c",
    "build:module": "npx babel -s --source-root 'node_modules/@10xjs/form/module' -d module --extensions '.tsx' src",
    "build:types": "npx tsc --emitDeclarationOnly -p build.tsconfig.json",
    "format": "npx prettier --write '{,!(coverage)/**/}{*.{js,tsx,html,md,yml},!(package-lock).json}'",
    "prepare": "npm run build",
    "test": "npm run test:types --slient && npm run test:lint --slient && npm run test:spec --silent",
    "test:lint": "npx eslint . --ext .tsx,.ts,.js.,.jsx",
    "test:spec": "npx jest --runInBand=${SPEC_SERIAL:-$CI} --coverage=${SPEC_COVERAGE:-$CI}",
    "test:spec-watch": "npm run spec -- --watch",
    "test:types": "npx tsc --noEmit"
  },
  "author": "Neal Granger <neal@nealg.com>",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./module/index.js",
  "types": "./types/index.d.js",
  "sideEffects": false,
  "files": [
    "lib/**/*.@(js|map)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/10xjs/form.git"
  },
  "dependencies": {
    "es6-error": "4.1.1"
  },
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-syntax-class-properties": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-flow-strip-types": "7.4.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.3",
    "@babel/plugin-transform-object-assign": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@babel/runtime": "7.4.3",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.13",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-jest": "24.7.1",
    "eslint": "5.16.0",
    "eslint-config-metalab": "10.0.0",
    "flow-bin": "0.71.0",
    "jest": "24.7.1",
    "ncp": "2.0.0",
    "prettier": "1.16.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "renamer": "1.1.1",
    "rimraf": "2.6.3",
    "rollup": "1.9.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.1",
    "typescript": "3.4.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "modulePathIgnorePatterns": [
      "lib/"
    ],
    "coveragePathIgnorePatterns": [
      "test/"
    ]
  }
}
