{
  "presets": [
    [
      "@babel/preset-env", {
        "modules": false,
        "shippedProposals": true,
      }
    ],
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "babel-plugin-dev-expression",
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs"
      ]
    },
    "lib": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs",
      ]
    },
    "es": {
      "plugins": []
    }
  }
}
